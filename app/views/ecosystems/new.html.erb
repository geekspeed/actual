<% content_for :title do %>
  <%= "Ecosystem" %>
<% end %>
<%= render "devise/shared/navbar", page: "about_me" %>
<%#= render "devise/shared/sidebar", page: "ecosystem" %>
<div id="rightcontent">
  <%= form_for @ecosystem do |f| %>
    <p>
      <%= f.label :name %>
      <%= f.text_field :name, :class => "required" %>
    </p>
    <p>
      <%= f.label :description %>
      <%= f.text_area :description %>
    </p>
    <p>
      <%= f.label :location %>
      <%= f.text_field :location %>
    </p>
    <p>
      <%= f.label :market %>
      <%= f.text_field :market %>
    </p>
    <p>
      <%= f.submit "Save", :id => "submit" %>
    </p>
  <% end %>
</div>