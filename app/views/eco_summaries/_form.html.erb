        </div>
        <div class="panel-body" style="padding-top:15px;">
          <table  id="programPlan" class="table table-bordered table-striped datatable dataTable" aria-describedby="table-2_info">
            <thead>
              <tr role="row">
                <th class="sorting" role="columnheader" tabindex="0" aria-controls="table-2" rowspan="1" colspan="1" style="width: 250px;" aria-label="Student Name: activate to sort column ascending">Date</th>
                <th class="sorting" role="columnheader" tabindex="0" aria-controls="table-2" rowspan="1" colspan="1" style="width: 290px;" aria-label="Average Grade: activate to sort column ascending">Time</th>
                <th class="sorting" role="columnheader" tabindex="0" aria-controls="table-2" rowspan="1" colspan="1" style="width: 450px;" aria-label="Average Grade: activate to sort column ascending">Activity</th>
                <th class="sorting" role="columnheader" tabindex="0" aria-controls="table-2" rowspan="1" colspan="1" style="width: 450px;" aria-label="Average Grade: activate to sort column ascending">Description</th>
                <th class="sorting" role="columnheader" tabindex="0" aria-controls="table-2" rowspan="1" colspan="1" style="width: 200px;" aria-label="Average Grade: activate to sort column ascending">Document</th>
                <th class="sorting" role="columnheader" tabindex="0" aria-controls="table-2" rowspan="1" colspan="1" style="width: 100px;" aria-label="Actions: activate to sort column ascending">Actions</th>
              </tr>
            </thead>
            
            <tbody role="alert" aria-live="polite" aria-relevant="all">
              <tr id="copy_col" class="odd no_display">
                <td class="">
                  <input class="datepicker_std eco_plan_td1 form-control form-style-1" id="eco_summary_eco_plans_attributes_-1-_date" name="eco_summary[eco_plans_attributes][-1-][date]"  type="text" />
                </td>
                <td class=" ">
                  <input class="eco_plan_td2 form-control form-style-1 time-picker" id="eco_summary_eco_plans_attributes_-1-_time_from" name="eco_summary[eco_plans_attributes][-1-][time_from]" placeholder="from"  type="text" />
                  <input class="eco_plan_td2 form-control form-style-1 time-picker" id="eco_summary_eco_plans_attributes_-1-_time_to" name="eco_summary[eco_plans_attributes][-1-][time_to]" placeholder="to"  style="margin-top: 4px" type="text" />
                        </td>
                <td class=" ">
                  <input class="eco_plan_td4 form-control form-style-1" id="eco_summary_eco_plans_attributes_-1-_activity" name="eco_summary[eco_plans_attributes][-1-][activity]"  type="text" />
                </td>
                <td class=" ">
                  <input class="eco_plan_td4 form-control form-style-1" id="eco_summary_eco_plans_attributes_-1-_location" name="eco_summary[eco_plans_attributes][-1-][location]"  type="text"/>
                </td>
                <td class=" ">
                  <input class="eco_plan_td5" id="eco_summary_eco_plans_attributes_-1-_attachment" name="eco_summary[eco_plans_attributes][-1-][attachment]" type="file">
                </td>
                <td class=" ">
                  &nbsp;
                </td>
              </tr>
              <%= f.fields_for :eco_plans, @summary.eco_plans.descending('created_at') do |plan| %>
              <tr class="odd">
                <td>
                  <%= plan.text_field :date, :class => "datepicker_std program_plan_td1 form-control form-style-1" %>
                </td>
                <td class=" ">
                  <%= plan.text_field :time_from, :placeholder => "from", :class => "program_plan_td2 time-picker form-control form-style-1" %>
                  <%= plan.text_field :time_to, :placeholder => "to", :class => "program_plan_td2 time-picker form-control form-style-1", :style => "margin-top: 4px" %>
                </td>
                <td class=" ">
                  <%= plan.text_field :activity, :class => "program_plan_td4 form-control form-style-1", :style => "height:71px;" %>
                </td>
                <td class=" ">
                  <%= plan.text_field :location, :class => "program_plan_td4 form-control form-style-1", :style => "height:71px;" %>
                </td>
                <td class=" ">
                  <%= plan.file_field :attachment, :class => "program_plan_td5 small_browse" %>
                </td>
                <td class=" ">
                  <%= link_to "<i class='glyphicon glyphicon-remove'></i> Delete".html_safe, eco_summaries_eco_plan_destroy_path(@program, eco_plan_id: plan.object), method: :delete, action: :eco_plan_destroy, class: "btn btn-danger btn-sm btn-icon icon-left"%>
                </td>
              </tr>
              <% end -%>
            </tbody>
          </table>
          <!-- <a href="#" class="newRow slink">+ Add new activity</a>  -->
          <!-- <a href="#" class="newCol slink">+ Add column</a> -->
          <input type="hidden" id="act_col" value="5" /> 
