<!-- This div is related to drag and drop positions. Classes are responsible for drag and drop are column and portlet. SO If you wants to add any of the codition inside the div, please add the condition inside the portlet class block and never add any thing outside the portlet class block. 1 block of column and 8 block of portlet-->
<div class="column">
  <div id="programs-1" class="row portlet">
    <div class="col-md-12">
      <div class="panel panel-white panel-shadow ui-widget collapses panel-table" style="margin-bottom:2px;" data-collapsed="0">
        <div class="panel-heading relative_panel_head portlet-header">
          <div class="panel-title title_option">
            <h3 style="padding-bottom: 10px;">
              <%= content_tag :div, class: "col-md-3", style: "width:100%" do %>
                <%= link_to "", "#", class: "js_inline_edit edit_btn glyphicon glyphicon-pencil", title:"Edit", style: "color: #FFFFFF !important;" %>
                <% programs_field = e("eco_summary:programs") ? e("eco_summary:programs") : "Programs"%>
                <span><%= content_tag :b, programs_field %></span>
                <%= text_field_tag "semantics[eco_summary:programs][singular]", programs_field , style: "display:none", class: "form-control input-size1 click_focus" %>
              <% end %>
              <span>
              <div class="radioset" style="width: 300px;">
                <div> 
                  <%= f.fields_for :eco_summary_customization, @summary.eco_summary_customization do |eco_summary_customization| %>
                    <%= eco_summary_customization.radio_button :programs, true %>
                    <%= eco_summary_customization.label :programs_true, "Enable" %>
                    <%= eco_summary_customization.radio_button :programs, false %>
                    <%= eco_summary_customization.label :programs_false, "Disable" %>
                  <% end %>
                </div>
              </div>
            </span>
          </div>
          <div class="panel-options">
            <a href="#" class="ui-expander gn-icon gn-icon-chevron-up" data-rel="collapse"></a>
          </div>
        </div>
        <div class="panel-body" style="padding-top:15px;">
          <% count=1 ; @organisation.programs.each do |program| %>
            <% if program.program_part_of_ecosystem %>
              <div class="row">
                <div class="col-md-12">
                  <div class="panel panel-white panel-shadow ui-widget collapses" data-collapsed="0"><!-- to apply shadow add class "panel-shadow" -->
                    <!-- panel head -->
                    <div class="panel-heading">
                      <div class="panel-title title_option">
                        <a class="user_name_panel pull-left"><%= program.title %></a> 
                      </div>
                      <div class="panel-options">
                        <% if need?(["company_admin", "program_admin"], current_organisation) %>
                          <%= link_to("", program_path(program), :class => "gn-icon icon-remove", method: :delete, confirm: "Are you sure. This can not be undone!", :style=>"font-size:12px;") %>
                        <% end %>
                        <a href="#" class="ui-expander gn-icon gn-icon-chevron-down" data-rel="collapse"></a>
                      </div>
                    </div>
                    
                    <!-- panel body -->
                    <div class="panel-body">
                      <div class="row user_data">
                        <div class="pull-left user_data_img_cotainer"><a><%= image_tag(program.try(:program_summary).try(:avatar).try(:logo).try(:url)) %></a></div>
                        <b>Goal:</b> <%= program.try(:program_summary).try(:goal).try(:html_safe) %>
                      </div>
                    </div>
                    
                  </div>
                
                </div>
              </div>
            <% end %>
          <% end %>
        </div><!-- panel-body -->
      </div>
    </div>
  </div>
  <div id="eco_plan-2" class="row portlet">
    <div class="col-md-12">
      <div class="panel panel-white panel-shadow ui-widget collapses panel-table" style="margin-bottom:2px;" data-collapsed="0">
        <div class="panel-heading relative_panel_head portlet-header">
