<%
=begin
%>

<%= link_to("#", :class => "open_div", :name => "new_mails") do %>
  <h2 class="func">Click here to update the feedback mail</h2>
<%- end -%>

<div id="new_mails" class="closable_div" style="display:none;">
  <div style="position: fixed;top: 213px;right: 189px;">
    <a class="btn btn-info" id="feedback-hint">* Hint</a>
  </div>
  <div style="position: fixed;top: 247px;right: 158px;">
    <div id="fedback-hintbox" style="display:none;margin-top:5px;background-color:#ffffff;padding:5px;">
      <div class="hint-title" rel="tooltip" title="name of person providing feedback">#from</div> 
      <div class="hint-title" rel="tooltip" title="name of person / people receiving feedback">#to</div> 
      <div class="hint-title" rel="tooltip" title="content of the feedback">#feedback</div> 
      <div class="hint-title" rel="tooltip" title="name of project receiving feedback">#projectname</div> 
      <div class="hint-title" rel="tooltip" title="name of program">#programname</div> 
      <div class="hint-title" rel="tooltip" title="name of company">#companyname</div> 
    </div>
  </div>
  <div class="block">

    <div class="inner">
      <h3>Admin Feedback Mail</h3>
      <p>Subject</p>
      <%= text_field_tag "subject[admin]" ,feedback_email_subject("admin","feedback"),:cols=>"40"%>
      <p>Body</p>
      <%= text_area_tag "feedback[admin]" ,feedback_email_description("admin","feedback"),:cols=>"40"%>
    </div>

    <div class="inner"><br/>
      <h3>Participant Feedback Mail</h3>
